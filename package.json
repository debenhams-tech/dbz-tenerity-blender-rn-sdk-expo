    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.4.6",
    "eslint": "^8.40.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^13.2.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react": "^17.0.2",
    "react-native": "^0.67.5",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "axios": "*",
    "react": "*",
    "react-native": "*",
    "react-native-webview": "*"
